[sub_resource type="Shader" id=2]
code = "shader_type canvas_item;

uniform sampler2D mask;

void fragment() {
	vec4 backkground_color = texture(SCREEN_TEXTURE, SCREEN_UV);
	vec4 color = texture(TEXTURE, UV);
	vec4 mask_color = texture(mask, UV);
	
	vec4 tex = backkground_color;
	
	float lum = (tex.r+tex.g+tex.b) * 0.3333;
	
	vec4 res = mix(vec4(vec3(lum), tex.a), tex, mask_color);
	// res *= color * (1.0 - mask_color);
	COLOR = mix(res, vec4(1.0, 0.0, 0.0, 1.0), vec4(vec3(0.0), 1.0));
}"

